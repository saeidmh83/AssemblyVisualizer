<UserControl x:Class="ILSpyVisualizer.AssemblyBrowser.UserControls.ScreenPresenter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             x:ClassModifier="internal"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             x:Name="userControl">
    <Grid>
        <ContentPresenter x:Name="cpGraph"/>
        <ContentPresenter x:Name="cpSearch"
                          Content="{Binding SearchView, ElementName=userControl}">
            <ContentPresenter.RenderTransform>
                <TranslateTransform x:Name="searchTransform"/>
            </ContentPresenter.RenderTransform>
        </ContentPresenter>
    </Grid>
</UserControl>
